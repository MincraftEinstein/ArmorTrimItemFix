{
  "schemaVersion": 1,
  "id": "armortrimitemfix",
  "version": "${version}",
  "name": "Armor Trim Item Fix",
  "description": "Trimmed armor items in Minecraft display one of four generic trim textures for every item, no matter what trim you put on an item the texture is always the same. This mod fixes that by adding more item trim textures that change depending on the trim applied",
  "authors": [
    "MincraftEinstein"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/armor-trim-item-fix",
    "sources": "https://github.com/MincraftEinstein/ArmorTrimItemFix",
    "issues": "https://github.com/MincraftEinstein/ArmorTrimItemFix/issues"
  },
  "license": "All rights reserved",
  "icon": "pack.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "einstein.armortrimitemfix.ArmorTrimItemFixFabric"
    ],
    "client": [
      "einstein.armortrimitemfix.ArmorTrimItemFixClient"
    ],
    "fabric-datagen": [
      "einstein.armortrimitemfix.ArmorTrimItemFixData"
    ]
  },
  "mixins": [
    "armortrimitemfix.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "minecraft": "1.20.x",
    "java": ">=17"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/gSsaFAvrBM"
      }
    }
  }
}
